<?xml version="1.0" encoding="utf-8"?>
<mx:Canvas xmlns:mx="http://www.adobe.com/2006/mxml" width="240" height="170"
	verticalScrollPolicy="off" horizontalScrollPolicy="off"
	xmlns:sync="com.xd.components.sync.*"
	xmlns:flash.filters="flash.filters.*"
	mouseDownOutside="PopUpManager.removePopUp(this)" >
	
	<mx:Script>
		<![CDATA[
			import mx.managers.PopUpManager;
		]]>
	</mx:Script>
	
	<mx:Box x="0" y="0" backgroundColor="#101010" width="100%" height="100%" >
		<mx:filters>
			<flash.filters:DropShadowFilter knockout="true" quality="4" distance="3" angle="90" blurX="8" blurY="8" color="0x000000" alpha="0.65" />
		</mx:filters>
	</mx:Box>
	
	<mx:Box x="0" y="0" backgroundColor="#101010" width="100%" height="100%" >
		<mx:filters>
			<flash.filters:DropShadowFilter knockout="true" quality="4" distance="1" angle="300" blurX="8" blurY="8" color="0x000000" alpha="0.45" />
		</mx:filters>
	</mx:Box>
	
	<sync:SynchSettingsDark x="0" y="0" />
	
</mx:Canvas>
